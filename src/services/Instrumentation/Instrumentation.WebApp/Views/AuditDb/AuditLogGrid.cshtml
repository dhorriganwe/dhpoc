@model Instrumentation.WebApp.Models.ViewQueryAuditLogList
@{
    Layout = null;
}

<div>
    <table>
        <tr>
            <td>count</td>
            <td>@Model.AuditLogs.Count</td>
        </tr>
    </table>
    <table class="alternate_color">
        <tr>
            <th>Id</th>
            <th>EventId</th>
            <th>ApplicationName</th>
            <th>FeatureName</th>
            <th>Category</th>
            <th>MessageCode</th>
            <th>Messages</th>
            <th>TraceLevel</th>
            <th>LoginName</th>
            <th>AuditedOn</th>
            <th>AdditionalInfo</th>
        </tr>
        @foreach (var auditLog in Model.AuditLogs)
        {
            <tr>
                <td>
                    <a href="AuditLogById/@auditLog.Id">@auditLog.Id</a>
                </td>
                <td class="eventId">
                    <a href="AuditLogByEventId/@auditLog.EventId">@auditLog.EventId</a>
                </td>
                <td>
                    <a href="AuditLogByApplicationName/@auditLog.ApplicationName">@auditLog.ApplicationName</a>
                </td>
                <td>
                    <a href="AuditLogByFeatureName/@auditLog.FeatureName">@auditLog.FeatureName</a>
                </td>
                <td>
                    <a href="AuditLogByCategory/@auditLog.Category">@auditLog.Category</a>
                </td>
                <td>@auditLog.MessageCode</td>
                <td class="messages">
                    <span title="@auditLog.Messages">@auditLog.Messages</span>
                </td>
                <td>@auditLog.TraceLevel</td>
                <td>@auditLog.LoginName</td>
                <td>@auditLog.AuditedOn</td>
                <td>@auditLog.AdditionalInfo</td>
            </tr>
        }
    </table>
</div>
