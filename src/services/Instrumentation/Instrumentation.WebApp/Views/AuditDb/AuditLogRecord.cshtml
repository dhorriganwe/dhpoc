@model Instrumentation.WebApp.Models.AuditLog
@{
    Layout = null;
}

<div>
    <table class="alternate_color">
        
        @if (!string.IsNullOrEmpty(Model.Id))
        {
            <tr><td>Id</td><td>@Model.Id</td></tr>
            <tr><td>EventId</td><td>@Model.EventId</td></tr>
            <tr><td>ApplicationName</td><td>@Model.ApplicationName</td></tr>
            <tr><td>FeatureName</td><td>@Model.FeatureName</td></tr>
            <tr><td>Category</td><td>@Model.Category</td></tr>
            <tr><td>TraceLevel</td><td>@Model.TraceLevel</td></tr>
            <tr><td>MessageCode</td><td>@Model.MessageCode</td></tr>
            <tr><td>AdditionalInfo</td><td>@Model.AdditionalInfo</td></tr>
            <tr><td>LoginName</td><td>@Model.LoginName</td></tr>
            <tr><td>AuditedOn</td><td>@Model.AuditedOn</td></tr>
            <tr><td>Messages</td><td>@Model.Messages</td></tr>
        }

    </table>
</div>
