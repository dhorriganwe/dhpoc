@model Instrumentation.WebApp.Models.AuditLogViewModel
@{
    Model.ViewTitle = "AuditLog";
}

<div class="PageRoot">
    
    @Html.Action("Header", "Home", Model)

    @using (@Html.BeginForm())
    {
        <div>
            <table class="alternate_color">
                <tr>
                    <td>MaxRowCount</td>
                    <td class="TextInputSmall">@Html.TextBoxFor(q => q.MaxRowCount, null, null)</td>
                    <td></td>
                    <td>DbKey</td>
                    <td>@Html.DropDownListFor(q => q.DbKey, Model.DbKeyList)</td>
                    <td></td>
                    <td>
                        <input type="submit" name="Command" value="Refresh" />
                    </td>
                </tr>
            </table>
        </div>

       
        @Html.Action("AuditLogGrid", Model)
    }
</div>